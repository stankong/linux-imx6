	arm-supply = <&sw1a_reg>;
	soc-supply = <&sw1c_reg>;
};
*/

&clks {
	fsl,ldb-di0-parent = <&clks IMX6QDL_CLK_PLL2_PFD0_352M>;
	fsl,ldb-di1-parent = <&clks IMX6QDL_CLK_PLL2_PFD0_352M>;
};

&ecspi5 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_ecspi5>;
	fsl,spi-num-chipselects = <2>;
	cs-gpios = <&gpio1 17 0>, <&gpio1 19 0>;
	status = "okay";

	flash@0 {
		compatible = "n25q032";
		spi-max-frequency = <20000000>;
		reg = <0>;
	};

	ads7846@1 {
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_ecspi5_ads7846_pin>;

		compatible = "ti,ads7846";
		vcc-supply = <&ads7846reg>;

		reg = <1>;				/* CS1 */
		spi-max-frequency = <1500000>;

		interrupt-parent = <&gpio5>;
		interrupts = <17 0>;			/* INT */
		pendown-gpio = <&gpio5 17 0>;

		ti,x-min = /bits/ 16 <0x0>;
		ti,x-max = /bits/ 16 <800>;
		ti,y-min = /bits/ 16 <0x0>;
		ti,y-max = /bits/ 16 <480>;

		ti,x-plate-ohms = /bits/ 16 <290>;
		ti,pressure-max = /bits/ 16 <600>;

		ti,debounce-max = /bits/ 16 <30>;
		ti,debounce-tol = /bits/ 16 <3>;
		ti,debounce-rep = /bits/ 16 <1>;

		linux,wakeup;
	};
};

&fec {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_enet>;
	phy-mode = "rgmii";
	phy-reset-gpios = <&gpio7 8 0>;
	fsl,magic-packet;
	status = "okay";
};

&can1 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_flexcan1>;
	status = "okay";
};

&can2 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_flexcan2>;
	status = "okay";
};

&gpc {
	fsl,ldo-bypass = <1>;
};

&dcic1 {
	dcic_id = <0>;
	dcic_mux = "dcic-hdmi";
	status = "okay";
};

&dcic2 {
	dcic_id = <1>;
	dcic_mux = "dcic-lvds1";
	status = "okay";
};

&hdmi_audio {
	status = "okay";
};

&hdmi_cec {
	status = "okay";
};

&hdmi_core {
	ipu_id = <0>;
	disp_id = <0>;
	status = "okay";
};

&hdmi_video {
	fsl,phy_reg_vlev = <0x0294>;
	fsl,phy_reg_cksymtx = <0x800d>;
	HDMI-supply = <&reg_hdmi>;
	status = "okay";
};

&i2c1 {
	clock-frequency = <100000>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c1>;
	status = "okay";

	rtc@68 {
		compatible = "dallas,ds1338";
		reg = <0x68>;
	};

	ov564x_mipi: ov564x_mipi@3c { /* i2c2 driver */
		compatible = "ovti,ov564x_mipi";
		reg = <0x3c>;
		clocks = <&clks 201>;
		clock-names = "csi_mclk";
		DOVDD-supply = <&vgen4_reg>; /* 1.8v */
		AVDD-supply = <&vgen3_reg>;  /* 2.8v, rev C board is VGEN3
						rev B board is VGEN5 */
		DVDD-supply = <&vgen2_reg>;  /* 1.5v*/
		pwn-gpios = <&gpio4 17 1>;   /* active low: SD1_CLK */
		rst-gpios = <&gpio4 16 0>;   /* active high: SD1_DAT2 */
		csi_id = <1>;
		mclk = <24000000>;
		mclk_source = <0>;
	};
};

&i2c2 {
	clock-frequency = <100000>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c2>;
	status = "okay";

	hdmi: edid@50 {
		compatible = "fsl,imx6-hdmi-i2c";
		reg = <0x50>;
	};
	
	codec: tlv320aic3x@18 {
		compatible = "ti,tlv320aic3x";
		reg = <0x18>;
		clocks = <&clks IMX6QDL_CLK_CKO>;
		ai3x-micbias-vg = <2>;
		clock-names = "mclk";
		IOVDD-supply = <&reg_audio>;
		DVDD-supply =  <&reg_audio_dvdd>;
		AVDD-supply =  <&reg_audio>;
		DRVDD-supply = <&reg_audio>;
	};
};

&i2c3 {
	clock-frequency = <100000>;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_i2c3>;
	status = "okay";

	capacitive_screen: ft5x06@38 {
		compatible = "edt,edt-ft5406", "edt,edt-ft5x06";
		reg = <0x38>;
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_i2c3_edt_ft5x06_int>;
		interrupt-parent = <&gpio4>;
		interrupts = <25 0>;
		reset-gpios = <&gpio4 27 1>;
	};
};

&iomuxc {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_hog>;

	imx6qdl-qiyang {
		pinctrl_hog: hoggrp {
			fsl,pins = <
				MX6QDL_PAD_GPIO_0__CCM_CLKO1    0x130b0		//TVL320AIC3101 MCLK
				MX6QDL_PAD_SD3_DAT7__GPIO6_IO17 0x80000000      //otg oc
				MX6QDL_PAD_GPIO_1__GPIO1_IO01 0x80000000    	//otg pwr
				//MX6QDL_PAD_SD2_CLK__GPIO1_IO10  0x80000000	//hub rst ---not designed on IAC-IMX6-MB-V1.2 board
				MX6QDL_PAD_SD3_RST__GPIO7_IO08 0x80000000 	//NET PHY reset
				
				MX6QDL_PAD_NANDF_CS0__GPIO6_IO11 0x80000000	//U29 LM4871 control
				MX6QDL_PAD_NANDF_CS1__GPIO6_IO14 0x80000000	//U30 LM4871 control
				MX6QDL_PAD_GPIO_2__GPIO1_IO02 0x80000000	//buzzer
			        MX6QDL_PAD_CSI0_PIXCLK__GPIO5_IO18 0x80000000	//feed_dog pin
				MX6QDL_PAD_CSI0_DATA_EN__GPIO5_IO20 0x80000000	//enable pin

				MX6QDL_PAD_DI0_DISP_CLK__GPIO4_IO16 0x80000000	//mipi reset
				MX6QDL_PAD_DI0_PIN3__GPIO4_IO19 0x80000000	//mipi enable
				MX6QDL_PAD_DI0_PIN15__GPIO4_IO17 0x80000000	//mipi power

				/*J16 gpio*/
				MX6QDL_PAD_GPIO_4__GPIO1_IO04 0x80000000
				MX6QDL_PAD_GPIO_7__GPIO1_IO07 0x80000000
				MX6QDL_PAD_GPIO_8__GPIO1_IO08 0x80000000
				MX6QDL_PAD_GPIO_9__GPIO1_IO09 0x80000000
				MX6QDL_PAD_GPIO_16__GPIO7_IO11 0x80000000
				MX6QDL_PAD_GPIO_17__GPIO7_IO12 0x80000000
				MX6QDL_PAD_GPIO_18__GPIO7_IO13 0x80000000
				MX6QDL_PAD_GPIO_19__GPIO4_IO05 0x80000000

				/*J27 gpio*/
				MX6QDL_PAD_DISP0_DAT3__GPIO4_IO24 0x80000000
				MX6QDL_PAD_DISP0_DAT5__GPIO4_IO26 0x80000000
				MX6QDL_PAD_DISP0_DAT7__GPIO4_IO28 0x80000000
				MX6QDL_PAD_DISP0_DAT9__GPIO4_IO30 0x80000000
				MX6QDL_PAD_DISP0_DAT11__GPIO5_IO05 0x80000000
				MX6QDL_PAD_DISP0_DAT13__GPIO5_IO07 0x80000000
				MX6QDL_PAD_DISP0_DAT15__GPIO5_IO09 0x80000000
				MX6QDL_PAD_DISP0_DAT17__GPIO5_IO11 0x80000000

			>;
		};

		pinctrl_audmux: audmuxgrp {
			fsl,pins = <
				MX6QDL_PAD_CSI0_DAT4__AUD3_TXC   		0x17088
				MX6QDL_PAD_CSI0_DAT5__AUD3_TXD			0x17088
				MX6QDL_PAD_CSI0_DAT6__AUD3_TXFS			0x17088
				MX6QDL_PAD_CSI0_DAT7__AUD3_RXD			0x17088
			>;
		};

		pinctrl_flexcan1: flexcan1grp {
			fsl,pins = <
				MX6QDL_PAD_KEY_ROW2__FLEXCAN1_RX        0x17059
				MX6QDL_PAD_KEY_COL2__FLEXCAN1_TX	0x17059
			>;
		};

		pinctrl_flexcan2: flexcan2grp {
			fsl,pins = <
				MX6QDL_PAD_KEY_COL4__FLEXCAN2_TX	0x17059
				MX6QDL_PAD_KEY_ROW4__FLEXCAN2_RX	0x17059
			>;
		};

		pinctrl_ecspi5_ads7846_pin: ads7846_pingrp {
			fsl,pins = <
				MX6QDL_PAD_DISP0_DAT23__GPIO5_IO17	0x80000000	//ads7846 int
			>;
		};

		pinctrl_ecspi5: ecspi5grp {
			fsl,pins = <
				MX6QDL_PAD_SD1_CMD__ECSPI5_MOSI		0x100b1
			        MX6QDL_PAD_SD1_CLK__ECSPI5_SCLK		0x100b1
			        MX6QDL_PAD_SD1_DAT0__ECSPI5_MISO	0x100b1
			        MX6QDL_PAD_SD1_DAT1__GPIO1_IO17		0x1b0b0	//cs0
			        MX6QDL_PAD_SD1_DAT2__GPIO1_IO19		0x1b0b0	//cs1
			>;
		};

		pinctrl_enet: enetgrp {
			fsl,pins = <
				MX6QDL_PAD_ENET_MDIO__ENET_MDIO		0x1b0b0
				MX6QDL_PAD_ENET_MDC__ENET_MDC		0x1b0b0
				MX6QDL_PAD_RGMII_TXC__RGMII_TXC		0x1b0b0
				MX6QDL_PAD_RGMII_TD0__RGMII_TD0		0x1b0b0
				MX6QDL_PAD_RGMII_TD1__RGMII_TD1		0x1b0b0
				MX6QDL_PAD_RGMII_TD2__RGMII_TD2		0x1b0b0
				MX6QDL_PAD_RGMII_TD3__RGMII_TD3		0x1b0b0
				MX6QDL_PAD_RGMII_TX_CTL__RGMII_TX_CTL	0x1b0b0
				MX6QDL_PAD_ENET_REF_CLK__ENET_TX_CLK	0x1b0b0
				MX6QDL_PAD_RGMII_RXC__RGMII_RXC		0x1b0b0
